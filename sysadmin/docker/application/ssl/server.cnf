[req]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn
x509_extensions = v3_req

[dn]
C=BE
ST=Limburg
L=Hasselt
O=Psychorama
OU=Internet services
emailAddress=info@psychorama.be
CN=davy.psychorama.test

[v3_req]
keyUsage=nonRepudiation, digitalSignature, keyEncipherment
extendedKeyUsage = serverAuth
subjectAltName = @alt_names

[alt_names]
DNS.1 = git.psychorama.test
DNS.2 = git-test.psychorama.test
